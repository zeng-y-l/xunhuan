{
  "name": "xunhuan",
  "version": "0.1.1",
  "type": "module",
  "description": "小而快的迭代器库",
  "keywords": ["iterator", "functional", "lazy", "collection", "util"],
  "author": "ZengYL <zeng-y-l@qq.com>",
  "license": "MIT",
  "repository": "github:zeng-y-l/xunhuan",
  "types": "src/index.ts",
  "module": "dist/index.js",
  "main": "dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "test": "vitest --coverage",
    "bench": "vitest bench",
    "fmt": "biome format --fix",
    "check": "biome lint --error-on-warnings && tsc",
    "compile": "vite build",
    "build": "pnpm run fmt && pnpm run check && pnpm run compile",
    "pub": "pnpm build && pnpm publish --registry=https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/lazy.js": "^0.5.9",
    "@types/lodash": "^4.17.16",
    "@vitest/coverage-v8": "3.1.1",
    "extra-iterable": "^3.3.2",
    "fast-check": "^4.1.0",
    "immutable": "^5.1.2",
    "iterare": "^1.2.1",
    "lazy.js": "^0.5.1",
    "lodash": "^4.17.21",
    "terser": "^5.39.0",
    "ts-pattern": "^5.7.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-doctest": "^1.1.1",
    "vitest": "~3.1.1"
  }
}
