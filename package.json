{
  "name": "xunhuan",
  "version": "0.3.0",
  "type": "module",
  "description": "小而快的迭代器库",
  "keywords": [
    "iterator",
    "functional",
    "lazy",
    "collection",
    "util"
  ],
  "author": "ZengYL <zeng-y-l@outlook.com>",
  "license": "MIT",
  "repository": "github:zeng-y-l/xunhuan",
  "types": "src/index.ts",
  "module": "dist/index.js",
  "main": "dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "vitest --coverage",
    "bench": "vitest bench",
    "fmt": "biome check --fix --linter-enabled=false",
    "check": "biome check --error-on-warnings && tsc",
    "compile": "vite build",
    "build": "pnpm run fmt && pnpm run check && pnpm run compile",
    "pub": "pnpm publish --registry=https://registry.npmjs.org/",

    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/lazy.js": "^0.5.9",
    "@types/lodash": "^4.17.20",
    "@vitest/coverage-v8": "3.2.4",
    "extra-iterable": "^3.3.2",
    "fast-check": "^4.2.0",
    "immutable": "^5.1.3",
    "iterare": "^1.2.1",
    "lazy.js": "^0.5.1",
    "lodash": "^4.17.21",
    "terser": "^5.43.1",
    "ts-pattern": "^5.8.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-doctest": "^1.1.1",
    "vitest": "~3.2.4"
  }
}
